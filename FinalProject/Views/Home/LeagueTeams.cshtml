@model FootballClubs

@{
    ViewData["Title"] = "LeagueTeams";
}

<h1>@Model.name</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.clubs[0].name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.clubs[0].country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.clubs[0].code)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.clubs) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.country)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.code)
            </td>
        </tr>
}
    </tbody>
</table>
